    <!-- https://annosaxfdm.de/ontology/derivedFrom -->

    <owl:ObjectProperty rdf:about="https://annosaxfdm.de/ontology/derivedFrom">
        <rdfs:domain rdf:resource="https://annosaxfdm.de/ontology/Phenotype"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://annosaxfdm.de/ontology/AnatomicalEntity"/>
                    <rdf:Description rdf:about="https://annosaxfdm.de/ontology/AnatomicalProperty"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>

    <!-- https://annosaxfdm.de/ontology/directionOf -->

    <owl:ObjectProperty rdf:about="https://annosaxfdm.de/ontology/directionOf">
        <rdfs:subPropertyOf rdf:resource="https://annosaxfdm.de/ontology/locationOf"/>
        <owl:inverseOf rdf:resource="https://annosaxfdm.de/ontology/hasDirection"/>
    </owl:ObjectProperty>


    <!-- https://annosaxfdm.de/ontology/hasDirection -->

    <owl:ObjectProperty rdf:about="https://annosaxfdm.de/ontology/hasDirection">
        <rdfs:subPropertyOf rdf:resource="https://annosaxfdm.de/ontology/hasLocation"/>
    </owl:ObjectProperty>


    <!-- https://annosaxfdm.de/ontology/hasLocation -->

    <owl:ObjectProperty rdf:about="https://annosaxfdm.de/ontology/hasLocation">
        <owl:inverseOf rdf:resource="https://annosaxfdm.de/ontology/locationOf"/>
        <rdfs:domain rdf:resource="https://annosaxfdm.de/ontology/AnatomicalEntity"/>
        <rdfs:range rdf:resource="https://annosaxfdm.de/ontology/RelativeAnatomicalLocation"/>
    </owl:ObjectProperty>


    <!-- https://annosaxfdm.de/ontology/locationOf -->

    <owl:ObjectProperty rdf:about="https://annosaxfdm.de/ontology/locationOf">
        <rdfs:domain rdf:resource="https://annosaxfdm.de/ontology/RelativeAnatomicalLocation"/>
        <rdfs:range rdf:resource="https://annosaxfdm.de/ontology/AnatomicalEntity"/>
    </owl:ObjectProperty>


    <!-- https://annosaxfdm.de/ontology/relativeTo -->

    <owl:ObjectProperty rdf:about="https://annosaxfdm.de/ontology/relativeTo">
        <rdfs:domain rdf:resource="https://annosaxfdm.de/ontology/RelativeAnatomicalLocation"/>
        <rdfs:range rdf:resource="https://annosaxfdm.de/ontology/AnatomicalEntity"/>
    </owl:ObjectProperty>


