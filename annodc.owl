        <foaf:homepage>https://annosaxfdm.de/</foaf:homepage>
        <foaf:logo>https://annosaxfdm.imise.uni-leipzig.de/public/anno-logo-blue.svg</foaf:logo>
    </owl:Ontology>

    <!--
